body{
	color       : var(--mainColor);
	font-weight : normal;
	font-style  : normal;
	font-size   : 16px;
	font-family : 'Roboto', sans-serif, system-ui;
}
.icon{
	display      : inline-block;
	fill         : currentColor;
	stroke       : currentColor;
	size         : 32px;
	stroke-width : 0;
}
.wrap{
	min-width: 100%;
}
